/* Genesis
--------------------------------------------- */

.breadcrumb {
  margin-bottom: 20px;
  margin-bottom: 2rem;
}

.archive-description,
.author-box {
  background-color: $white;
  margin-bottom: 40px;
  margin-bottom: 4rem;
}

.archive-description {
  padding: 40px 40px 24px;
  padding: 4rem 4rem 2.4rem;
}

.author-box {
  padding: 40px;
  padding: 4rem;
}

.author-box-title {
  color: $darkgray;
  font-family: 'Roboto Slab', serif;
  font-size: 16px;
  font-size: 1.6rem;
  font-weight: 400;
  margin-bottom: 4px;
  margin-bottom: 0.4rem;
}

.author-box {
