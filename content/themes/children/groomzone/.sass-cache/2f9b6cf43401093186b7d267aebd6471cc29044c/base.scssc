/* Titles
--------------------------------------------- */

.entry-title {
  font-size: 36px;
  font-size: 3.6rem;
  line-height: 1;
  a {
    color: $darkgray;
  }
}

.sidebar .widget-title a {
  color: $darkgray;
}

.entry-title a:hover {
  color: $medgray;
}

.widget-title {
  font-size: 16px;
  font-size: 1.6rem;
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
  text-transform: uppercase;
}

.sidebar .widget-title a {
  border: none;
}

.archive-title {
  font-size: 20px;
  font-size: 2rem;
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
}

/*
Widgets
---------------------------------------------------------------------------------------------------- */
 
.widget {
  word-wrap: break-word;
}

/* Featured Content
--------------------------------------------- */

.featured-content {
  .entry {
  }
  .entry-title {
    border: none;
    font-size: 24px;
    font-size: 2.4rem;
    line-height: 1.2;
    a {
      border: none;
    }
  }
}

/*
Plugins
---------------------------------------------------------------------------------------------------- */

/* Genesis eNews Extended
--------------------------------------------- */

.enews-widget {
  color: $gray;
  .widget-title {
    color: $white;
  }
  input {
    margin-bottom: 16px;
    margin-bottom: 1.6rem;
    &:focus {
      border: 1px solid #ddd;
    }
    &[type="submit"] {
      background-color: #f15123;
      color: $white;
      margin: 0;
      width: 100%;
    }
    &:hover[type="submit"] {
      background-color: $white;
      color: $darkgray;
    }
  }
}

/* Genesis Latest Tweets
--------------------------------------------- */

.latest-tweets ul li {
  margin-bottom: 16px;
  margin-bottom: 1.6rem;
}

/* Gravity Forms
--------------------------------------------- */

div.gform_wrapper {
  input {
    &[type="email"], &[type="text"] {
      font-size: 16px;
      font-size: 1.6rem;
      padding: 16px;
      padding: 1.6rem;
    }
  }
  textarea {
    font-size: 16px;
    font-size: 1.6rem;
    padding: 16px;
    padding: 1.6rem;
  }
  .ginput_complex label {
    font-size: 16px;
    font-size: 1.6rem;
    padding: 16px;
    padding: 1.6rem;
    padding: 0;
  }
  li, form li {
    margin: 16px 0 0;
    margin: 1.6rem 0 0;
  }
  .gform_footer input[type="submit"] {
    font-size: 14px;
    font-size: 1.4rem;
  }
}

/* Jetpack
--------------------------------------------- */

img#wpstats {
  display: none;
}

/*
Site Header
---------------------------------------------------------------------------------------------------- */

.site-header {
  float: left;
  position: fixed;
  text-align: center;
  width: 220px;
  p {
    margin: 0;
  }
  .site-title::after, .widget::after {
    border-bottom: 1px dotted $darkgray;
    content: "";
    display: block;
    margin: 0 auto 30px;
    margin: 0 auto 3rem;
    width: 25%;
  }
  .site-title::after {
    padding-bottom: 35px;
    padding-bottom: 3.5rem;
  }
  .widget::after {
    padding-bottom: 30px;
    padding-bottom: 3rem;
  }
  .simple-social-icons ul li {
    margin: 0 5px !important;
    margin: 0 0.5rem !important;
    a {
      padding: 10px !important;
      padding: 1rem !important;
    }
  }
  ul li {
    list-style-type: none;
  }
  .wrap {
  }
}

/* Title Area
--------------------------------------------- */

.site-title {
  font-family: 'Roboto Slab', serif;
  font-size: 30px;
  font-size: 3rem;
  line-height: 1;
  margin-bottom: 0;
}

.site-header a {
  border: none;
}

.title-area {
  padding-top: 15px;
}

.header-image {
  .site-title a {
    /* 9999px HACK FIX
    http://www.zeldman.com/2012/03/01/replacing-the-9999px-hack-new-image-replacement/ */
    background: url(../../images/logo.png) no-repeat left;
    text-indent: 100%;
    white-space: nowrap;
    overflow: hidden;
  }
  .site-header .wrap {
    padding: 0;
  }
  .site-title a {
    float: left;
    min-height: 230px;
    width: 100%;
  }
  .site-description {
  display: none;
  }
}


/* Widget Area
--------------------------------------------- */

.widget-area {
  word-wrap: break-word;
}

.site-header .widget-area {
  text-align: center;
  a {
    font-size: 30px;
    font-size: 3rem;
  }
}

.header-image .site-header .widget-area {
  padding: 40px 0;
  padding: 4rem 0;
}

.site-header .search-form {
  float: right;
  margin-top: 24px;
  margin-top: 2.4rem;
}

/*
Site Navigation
---------------------------------------------------------------------------------------------------- */

.nav-primary {
  background-color: #000;
  margin-bottom: 30px;
  margin-bottom: 3rem;
  .genesis-nav-menu {
    clear: both;
    font-family: 'Roboto Condensed', sans-serif;
    font-weight: 300;
    line-height: 1.5;
    width: 100%;
    .menu-item {
      display: inline-block;
      text-align: left;
    }
    a {
      border: none;
      color: #fff;
      display: block;
      font-size: 30px;
      font-size: 3rem;
      font-weight: 300;
      padding: 10px 20px;
      padding: 1rem 2rem;
      position: relative;
      &:hover {
        background-color: #fff;
        color: #000;
      }
    }
    .current-menu-item > a {
      background-color: #fff;
      color: #000;
    }
    .sub-menu {
      a, .current-menu-item > a:hover {
        background-color: #fff;
        color: #000;
      }
      a {
        &:hover {
          background-color: #000;
          color: #fff;
        }
        background-color: #fff;
        border: 1px solid #eee;
        border-top: none;
        font-size: 14px;
        font-size: 1.4rem;
        padding: 10px 20px;
        padding: 1rem 2rem;
        position: relative;
        width: 180px;
      }
      left: -9999px;
      margin-left: -1px;
      margin-left: -0.1rem;
      opacity: 0;
      position: absolute;
      -webkit-transition: opacity .4s ease-in-out;
      -moz-transition: opacity .4s ease-in-out;
      -ms-transition: opacity .4s ease-in-out;
      -o-transition: opacity .4s ease-in-out;
      transition: opacity .4s ease-in-out;
      width: 180px;
      z-index: 99;
      .sub-menu {
        margin: -42px 0 0 179px;
      }
    }
    .menu-item:hover {
      position: static;
      > .sub-menu {
        left: auto;
        opacity: 1;
      }
    }
    > {
      .right {
        color: #fff;
        float: right;
        list-style-type: none;
        display: inline-block;
        a {
          display: inline-block;
        }
        > a:hover {
          background: none;
          color: #aaa;
        }
        &.date {
          padding: 10px 20px;
          padding: 1rem 2rem;
        }
      }
      .search {
        margin-right: 20px;
        margin-right: 2rem;
        padding: 1px;
        padding: 0.1rem;
      }
    }
  }
  .search-form {
    width: 180px;
  }
}

/* Responsive Menu
--------------------------------------------- */

.responsive-menu-icon {
  cursor: pointer;
  display: none;
  &::before {
    -webkit-font-smoothing: antialiased;
    color: #fff;
    content: "\e00e";
    font-family: 'Icon';
    font-size: 16px;
    font-size: 1.6rem;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    line-height: 1;
    margin: 0 auto;
    speak: none;
    text-transform: none;
  }
}

.site-header .responsive-menu-icon::before {
  color: #000;
}

/*
Content Area
---------------------------------------------------------------------------------------------------- */

/* Entries
--------------------------------------------- */

.entry {
  margin-bottom: 16px;
  margin-bottom: 1.6rem;
  padding: 16px 16px 16px;
  padding: 1.6rem 1.6rem 1.6rem;
}

.entry-content {
  a {
    border-bottom: 1px solid #ddd;
    color: #f15123;
    &:hover {
      color: $darkgray;
    }
  }
  .attachment a,
  .gallery a {
    border: none;
  }
  ol, p, ul {
    margin-bottom: 26px;
    margin-bottom: 2.6rem;
  }
}

.quote-caption {
  margin-bottom: 26px;
  margin-bottom: 2.6rem;
}

.entry-content {
  ol, ul {
    margin-left: 40px;
    margin-left: 4rem;
  }
  ol li {
    list-style-type: decimal;
  }
  ul li {
    list-style-type: disc;
  }
  ol ol, ul ul {
    margin-bottom: 0;
  }
  .search-form {
    width: 50%;
  }
}

.entry-meta {
  color: $gray;
  font-family: 'Roboto Condensed', sans-serif;
  font-size: 14px;
  font-size: 1.4rem;
  a {
    border-bottom: 1px solid $gray;
  }
}

.entry-header .entry-meta {
  margin-bottom: 24px;
  margin-bottom: 2.4rem;
}

.entry-footer .entry-meta {
  border-top: 2px solid #f5f5f5;
  margin: 0 -40px;
  margin: 0 -4rem;
  padding: 32px 40px 8px;
  padding: 3.2rem 4rem 0.8rem;
}

.entry-comments-link::before {
  content: "\2014";
  margin: 0 6px 0 2px;
  margin: 0 0.6rem 0 0.2rem;
}

.entry-categories,
.entry-tags {
  display: block;
}

/* Pagination
--------------------------------------------- */

.archive-pagination,
.entry-pagination {
  font-size: 14px;
  font-size: 1.4rem;
  margin: 40px 0;
  margin: 4rem 0;
}

.archive-pagination li {
  display: inline;
  a {
    background-color: $darkgray;
    border-radius: 3px;
    color: $white;
    cursor: pointer;
    display: inline-block;
    font-family: 'Roboto Condensed', sans-serif;
    font-size: 16px;
    font-size: 1.6rem;
    font-weight: 300;
    padding: 8px 12px;
    padding: 0.8rem 1.2rem;
    text-transform: uppercase;
    &:hover {
      background-color: #f15123;
    }
  }
  &.active a {
    background-color: #f15123;
  }
}

.entry-pagination a {
  border-bottom: 1px solid #ddd;
}

/* Comments
--------------------------------------------- */

p.comment-meta {
  margin-bottom: 40px;
  margin-bottom: 4rem;
}

.entry-comments {
  margin-bottom: 40px;
  margin-bottom: 4rem;
  .comment, .comment-reply {
    margin-bottom: 40px;
    margin-bottom: 4rem;
  }
}

.ping-list {
  margin-bottom: 40px;
  margin-bottom: 4rem;
}

.entry-comments .comment {
  border-bottom: 1px solid #000;
}

p.form-submit {
  margin: 0;
}

.entry-comments {
  .children .comment, .comment-author {
    margin: 0;
  }
  .comment {
    clear: both;
  }
  .children .comment {
    border: none;
  }
}

.comment-respond {
  label {
    display: block;
  }
  input {
    &[type="email"], &[type="text"], &[type="url"] {
      width: 50%;
    }
  }
}

.comment-list li, .ping-list li {
  list-style-type: none;
}

ul.children {
  margin-left: 40px;
  margin-left: 4rem;
}

.form-allowed-tags, .ping-list .reply {
  display: none;
}

/*
Site Footer
---------------------------------------------------------------------------------------------------- */

.site-footer {
  font-family: 'Roboto Condensed', sans-serif;
  font-size: 12px;
  font-size: 1.25rem;
  text-align: center;
  a {
    border: none;
    font-weight: 300;
  }
}
