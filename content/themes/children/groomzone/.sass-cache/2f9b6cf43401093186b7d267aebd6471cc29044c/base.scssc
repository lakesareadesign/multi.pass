  margin: 0 0 2.4rem 2.4rem;
}

.wp-caption-text {
  font-size: 14px;
  font-size: 1.4rem;
  font-weight: 700;
  text-align: center;
}

.gallery-caption! {}

.widget_calendar {
  table {
